<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
		android:id="@+id/relativeLayout1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		xmlns:android="http://schemas.android.com/apk/res/android">

	<TextView
			android:id="@+id/list_logs_user"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="fill_parent"
			android:text="@string/dummy_username"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/list_logs_type"
			android:layout_margin="2dip"
			android:gravity="fill_horizontal"/>

	<ImageView
			android:layout_height="wrap_content"
			android:src="@drawable/found"
			android:id="@+id/list_logs_type"
			android:layout_margin="2dip"
			android:layout_width="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_alignParentLeft="true"></ImageView>

	<TextView
			android:layout_height="wrap_content"
			android:id="@+id/list_logs_date"
			android:layout_width="wrap_content"
			android:text="@string/dummy_date"
			android:layout_margin="2dip"
			android:layout_alignParentTop="true"
			android:layout_alignParentRight="true"></TextView>

	<TextView
			android:layout_height="wrap_content"
			android:id="@+id/list_logs_body"
			android:layout_width="wrap_content"
			android:text="@string/dummy_lorem"
			android:layout_margin="2dip"
			android:layout_below="@+id/list_logs_type"
			android:layout_alignLeft="@+id/list_logs_type"></TextView>

</RelativeLayout>
